<!-- eahv-iv-2402-000401 edited with XMLSpy v2013 sp1 (http://www.altova.com) by Patrick Blaser -->
<xs:schema xmlns:eCH-0010="http://www.ech.ch/xmlns/eCH-0010-f/5" xmlns:eCH-0044="http://www.ech.ch/xmlns/eCH-0044-f/4"
	 xmlns:eCH-0058="http://www.ech.ch/xmlns/eCH-0058/4" xmlns:eahv-iv-2402-common="http://www.eahv-iv.ch/xmlns/eahv-iv-2402-common/2"
	 xmlns:eahv-iv-common="http://www.eahv-iv.ch/xmlns/eahv-iv-common/4" xmlns:eahv-iv-2402-000401="http://www.eahv-iv.ch/xmlns/eahv-iv-2402-000401/2"
	 xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.eahv-iv.ch/xmlns/eahv-iv-2402-000401/2"
	 elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0">
	<xs:annotation>
		<xs:documentation xml:lang="de">Abschliessen Mehrfachtätigkeit</xs:documentation>
		<xs:documentation xml:lang="fr">Conclure activité multiple</xs:documentation>
		<xs:documentation xml:lang="it">Concludere attività multiple</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0010-f/5"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0044-f/4"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0058/4"/>
	<xs:import namespace="http://www.eahv-iv.ch/xmlns/eahv-iv-2402-common/2"/>
	<xs:import namespace="http://www.eahv-iv.ch/xmlns/eahv-iv-common/4"/>
	<xs:complexType name="headerType">
		<xs:sequence>
			<xs:element name="senderId" type="eCH-0058:participantIdType"/>
			<xs:element name="recipientId" type="eCH-0058:participantIdType"/>
			<xs:element name="messageId" type="eCH-0058:messageIdType"/>
			<xs:element name="referenceMessageId" type="eCH-0058:messageIdType" minOccurs="0"/>
			<xs:element name="businessProcessId" type="eCH-0058:businessProcessIdType"/>
			<xs:element name="ourBusinessReferenceId" type="eCH-0058:businessReferenceIdType"/>
			<xs:element name="yourBusinessReferenceId" type="eCH-0058:businessReferenceIdType" minOccurs="0"/>
			<xs:element name="messageType" type="eCH-0058:messageTypeType" fixed="2402"/>
			<xs:element name="subMessageType" type="eCH-0058:subMessageTypeType" fixed="000401"/>
			<xs:element name="sendingApplication" type="eCH-0058:sendingApplicationType"/>
			<xs:element name="subject" type="eCH-0058:subjectType"/>
			<xs:element name="comment" type="eCH-0058:commentType" minOccurs="0"/>
			<xs:element name="messageDate" type="eCH-0058:messageDateType"/>
			<xs:element name="action">
				<xs:simpleType>
					<xs:restriction base="eCH-0058:actionType">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">Neu</xs:documentation>
								<xs:documentation xml:lang="fr">Nouveau</xs:documentation>
								<xs:documentation xml:lang="it">Nuovo</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="attachment" type="eahv-iv-2402-common:attachmentType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="testDeliveryFlag" type="eCH-0058:testDeliveryFlagType"/>
			<xs:element name="responseExpected" type="eCH-0058:responseExpectedType" fixed="false"/>
			<xs:element name="businessCaseClosed" type="eCH-0058:businessCaseClosedType" fixed="true"/>
			<xs:element name="extension" type="eahv-iv-2402-common:extensionType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contentType">
		<xs:sequence>
			<xs:element name="insuredPerson" type="eahv-iv-common:naturalPersonsOASIDIType"/>
			<xs:element name="vn" type="eCH-0044:vnType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Sozialversicherungsnummer der Schweiz (AHV-Nr)</xs:documentation>
					<xs:documentation xml:lang="fr">Numéro de sécurité sociale en Suisse (N° AVS)</xs:documentation>
					<xs:documentation xml:lang="it">Numero di sicurezza sociale in Svizzera (N. AVS)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="officialName" type="eCH-0044:baseNameType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Name</xs:documentation>
					<xs:documentation xml:lang="fr">Nom</xs:documentation>
					<xs:documentation xml:lang="it">Cognome</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="birthName" type="eCH-0044:baseNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Geburtsname</xs:documentation>
					<xs:documentation xml:lang="fr">Nom de naissance</xs:documentation>
					<xs:documentation xml:lang="it">Cognome di nascita</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstName" type="eCH-0044:baseNameType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Vorname</xs:documentation>
					<xs:documentation xml:lang="fr">Prénom</xs:documentation>
					<xs:documentation xml:lang="it">Nome</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sex" type="eCH-0044:sexType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Geschlecht</xs:documentation>
					<xs:documentation xml:lang="fr">Sexe</xs:documentation>
					<xs:documentation xml:lang="it">Sesso</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateOfBirth" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Geburtsdatum - Datumsformat: YYYY-MM-DD (zB. 1961-01-11                        für 11ter Januar)</xs:documentation>
					<xs:documentation xml:lang="fr">Date de naissance - Format de la date: AAAA-MM-JJ (p.ex. 1961-01-11 pour 11 janvier)</xs:documentation>
					<xs:documentation xml:lang="it">Data di nascita - Formato della data: AAAA-MM-GG (p.es. 1961-01-11 per 11 gennaio)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="placeOfBirth" type="eCH-0044:baseNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Geburtsort</xs:documentation>
					<xs:documentation xml:lang="fr">Lieu de naissance</xs:documentation>
					<xs:documentation xml:lang="it">Luogo di nascita</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="citizenshipCountry" type="eCH-0010:countryIdIso2Type" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Staatsangehörigkeit(en)</xs:documentation>
					<xs:documentation xml:lang="fr">Nationalité(s)</xs:documentation>
					<xs:documentation xml:lang="it">Nazionalità</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employeeResidenceAddress" type="eahv-iv-2402-common:addressInformationStrictType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Adresse im Wohnsitzstaat</xs:documentation>
					<xs:documentation xml:lang="fr">Adresse dans l'Etat de résidence</xs:documentation>
					<xs:documentation xml:lang="it">Indirizzo nello Stato di residenza</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="swissAddress" type="eahv-iv-2402-common:addressInformationStrictType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Adresse in der Schweiz, falls unterschiedlich von                        Adresse in Wohnstaat</xs:documentation>
					<xs:documentation xml:lang="fr">Adresse en Suisse si différente de l'adresse dans l'Etat de résidence</xs:documentation>
					<xs:documentation xml:lang="it">Indirizzo in Svizzera se differente dall'indirizzo nello Stato di residenza</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="occupation" type="eahv-iv-2402-common:occupationType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Tätigkeiten</xs:documentation>
					<xs:documentation xml:lang="fr">Activités</xs:documentation>
					<xs:documentation xml:lang="it">Attività</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deploymentDecision" type="eahv-iv-2402-common:deploymentDecisionType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entscheid zur Mehrfachtätigkeit</xs:documentation>
					<xs:documentation xml:lang="fr">Décision sur l'activité multiple</xs:documentation>
					<xs:documentation xml:lang="it">Decisione sull'attività multiple</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="message">
		<xs:annotation>
			<xs:documentation xml:lang="de">Deklaration des Root-Elements für message.xml</xs:documentation>
			<xs:documentation xml:lang="fr">Déclaration de l'élément Root pour message.xml</xs:documentation>
			<xs:documentation xml:lang="it">Dichiarazione dell'elemento Root per message.xml</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="header" type="eahv-iv-2402-000401:headerType"/>
				<xs:element name="content" type="eahv-iv-2402-000401:contentType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="minorVersion" type="xs:integer" use="required"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
