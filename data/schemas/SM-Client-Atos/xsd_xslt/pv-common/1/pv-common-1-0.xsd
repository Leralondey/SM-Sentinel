	<xs:simpleType name="agencyTypeForUnion">
		<xs:annotation>
			<xs:documentation>Enthält die Enumeration GEKVG zum Ergänzung des Typs eCH-0007 im Typ agencyType</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="GEKVG"/>
