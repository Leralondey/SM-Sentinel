<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eCH-0007="http://www.ech.ch/xmlns/eCH-0007/4" xmlns:eCH-0008="http://www.ech.ch/xmlns/eCH-0008/2" xmlns:eCH-0010="http://www.ech.ch/xmlns/eCH-0010/4" xmlns:eCH-0044="http://www.ech.ch/xmlns/eCH-0044/2" xmlns:eCH-0058="http://www.ech.ch/xmlns/eCH-0058/4" xmlns:eCH-0097="http://www.ech.ch/xmlns/eCH-0097/1" xmlns:da-64a-common="http://www.gdk-cds.ch/xmlns/da-64a-common/1" targetNamespace="http://www.gdk-cds.ch/xmlns/da-64a-common/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0">
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0007/4"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0008/2"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0010/4"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0044/2"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0058/4"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0097/1"/>
	<xs:annotation>
		<xs:documentation xml:lang="de">Datenaustausch 64a: Basistypen für die Meldungen</xs:documentation>
	</xs:annotation>
	<!-- ============================================= header type ============================================= -->
	<xs:complexType name="headerType" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="de">Ableitung von eCH-0058:headerType.
 Dieser Typ kann nicht direkt benutzt werden. Er muss für jede Meldung abgeleitet werden.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="senderId" type="eCH-0058:participantIdType"/>
			<xs:element name="originalSenderId" type="eCH-0058:participantIdType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="declarationLocalReference" type="eCH-0058:declarationLocalReferenceType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="recipientId" type="eCH-0058:participantIdType"/>
			<xs:element name="messageId" type="eCH-0058:messageIdType"/>
			<xs:element name="referenceMessageId" type="eCH-0058:messageIdType" minOccurs="0"/>
			<xs:element name="businessProcessId" type="da-64a-common:businessProcessIdType"/>
			<xs:element name="ourBusinessReferenceId" type="eCH-0058:businessReferenceIdType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="yourBusinessReferenceId" type="eCH-0058:businessReferenceIdType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="uniqueIdBusinessTransaction" type="eCH-0058:uniqueIdBusinessTransactionType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="messageType" type="eCH-0058:messageTypeType"/>
			<xs:element name="subMessageType" type="eCH-0058:subMessageTypeType"/>
			<xs:element name="sendingApplication" type="eCH-0058:sendingApplicationType"/>
			<xs:element name="partialDelivery" type="eCH-0058:partialDeliveryType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="subject" type="eCH-0058:subjectType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="comment" type="eCH-0058:commentType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="messageDate" type="eCH-0058:messageDateType"/>
			<xs:element name="initialMessageDate" type="eCH-0058:messageDateType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="eventDate" type="eCH-0058:eventDateType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="modificationDate" type="eCH-0058:eventDateType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="action" type="da-64a-common:actionType"/>
			<xs:element name="testDeliveryFlag" type="eCH-0058:testDeliveryFlagType"/>
			<xs:element name="responseExpected" type="eCH-0058:responseExpectedType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="businessCaseClosed" type="eCH-0058:businessCaseClosedType" minOccurs="0" maxOccurs="0"/>
			<xs:element name="extension" type="da-64a-common:extensionType" minOccurs="0" maxOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="actionType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:minLength value="1"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="de">Neue Meldung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation xml:lang="de">Anfrage</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation xml:lang="de">Antwort</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Von eCH-0046 übernommen, da eCH-0046-2-0.xsd auf die non-forgiving Version von eCH-0010 und eCH-0044 verweist, in DA-PV aber die forgiving Version benutzt wird.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="\d{10,20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="emailAddressType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Von eCH-0046 übernommen, da eCH-0046-2-0.xsd auf die non-forgiving Version von eCH-0010 und eCH-0044 verweist, in DA-PV aber die forgiving Version benutzt wird.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="100"/>
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+(\.[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+)*@[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+(\.[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+)*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="businessProcessIdType">
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="extensionType">
		<xs:annotation>
			<xs:documentation>Enthält das Element "contactInformation"</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="contactInformation" type="da-64a-common:contactInformationType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contactInformationType">
		<xs:sequence>
			<xs:element name="name" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="department" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Abteilung</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="phone" type="da-64a-common:phoneNumberType">
				<xs:annotation>
					<xs:documentation>Telefon</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="email" type="da-64a-common:emailAddressType">
				<xs:annotation>
					<xs:documentation>E-Mail-Adresse</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="other" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Freitext</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ============================================= content types ============================================= -->
	<!-- For 64a 5231-000101 message -->
	<xs:complexType name="debtCollectionEventType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Betreibungsereignis</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="debtCollectionEventEnum">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betreibungsereignis, das gemeldet wird</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">Betreibungsbegehren</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="de">Fortsetzungsbegehren möglich</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="de">Fortsetzungsbegehren gestellt</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="4">
							<xs:annotation>
								<xs:documentation xml:lang="de">Verlustschein</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="5">
							<xs:annotation>
								<xs:documentation xml:lang="de">Vollzahlung</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="6">
							<xs:annotation>
								<xs:documentation xml:lang="de">Annullation</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="7">
							<xs:annotation>
								<xs:documentation xml:lang="de">Inkasso EU/EFTA</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="debtCollectionEventDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum des auslösenden Ereignisses</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="collectionOffice" type="da-64a-common:collectionOfficeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betreibungsamt zum zugehörigen Betreibungsereignis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseNumber" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betreibungsnummer</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="certificateOfLoss" type="da-64a-common:certificateOfLossType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="de">Verlustschein</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtorWithClaim" type="da-64a-common:debtorWithClaimType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Schuldner mit den dazugehörigen Forderungen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="insuredPersonWithClaim" type="da-64a-common:insuredPersonWithClaimType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Versicherte Personen mit den dazugehörigen Forderungen</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="addressType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Domiziladresse</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="addressLine1" type="eCH-0010:addressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Adresszusatzzeile für personifizierte Adressangaben (z.B. c/o-Adresse)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addressLine2" type="eCH-0010:addressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Adresszusatzzeile für unpersonifizierte Adressangaben</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="street" type="eCH-0010:streetType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Strassenbezeichnungen (dies kann auch der Name einer Lokalität, etc. sein)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="houseNumber" type="eCH-0010:houseNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hausnummer (zu setzen, falls vorhanden)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="town" type="eCH-0010:townType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ortsname (in ausländischen Adressen falls nötig inkl. Provinz etc.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="swissZipCode" type="eCH-0010:swissZipCodeType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Schweizer Postleitzahl (zu setzten, falls Land CH)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="foreignZipCode" type="eCH-0010:foreignZipCodeType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Ausländische Postleitzahl (zu setzten, falls Ausland)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="country" type="eCH-0008:countryIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Land, vierstelliger BFS-Ländercode</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="municipalityName" type="eCH-0007:municipalityNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gemeindename Domizil</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="municipalityId" type="eCH-0007:municipalityIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gemeindenummer Domizil (BFS-Gemeindecode)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="assumptionOfClaimsType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Forderungsübernahme</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="assumptionDecision" type="xs:boolean">
				<xs:annotation>
					<xs:documentation xml:lang="de">Forderungsübernahme ja/nein</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtor" type="da-64a-common:debtorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Schuldner und Adresse</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="certificateOfLossQuarterlyStatementType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Quartalsmeldung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="statementDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Erstelldatum der Meldung (Datum des Datenabzugs des Krankenversicherers)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statementStartDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Startdatum des zu betrachtenden Zeitraums</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statementEndDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Enddatum des zu betrachtenden Zeitraums</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="certificateOfLossArrival" type="da-64a-common:certificateOfLossArrivalType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Eingegangene Verlustscheine oder gleichzusetzende Rechtstitel, welche dem Kanton in Rechnung gestellt werden</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalClaim">
				<xs:annotation>
					<xs:documentation xml:lang="de">Totalbetrag (Summe) der eingegangenen Verlustscheine oder gleichzusetzender Rechtstitel in CHF (nicht gerundet) abzüglich allfälliger Teilzahlungen</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="certificateOfLossFinalStatementType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Schlussabrechnung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="statementDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Erstelldatum der Meldung (Datum des Datenabzugs des Krankenversicherers)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statementStartDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Startdatum des zu betrachtenden Zeitraums</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statementEndDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Enddatum des zu betrachtenden Zeitraums</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="certificateOfLossArrival" type="da-64a-common:certificateOfLossArrivalType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Eingegangene Verlustscheine oder gleichzusetzende Rechtstitel, welche dem Kanton in Rechnung gestellt werden</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="certificateOfLossPayment" type="da-64a-common:certificateOfLossPaymentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zahlungseingänge von Verlustscheinen oder gleichzusetzenden Rechtstiteln, welche dem Kanton bereits in Rechnung gestellt wurden</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalClaim">
				<xs:annotation>
					<xs:documentation xml:lang="de">Totalbetrag (Summe) der eingegangenen Verlustscheine oder gleichzusetzender Rechtstitel in CHF (nicht gerundet) abzüglich allfälliger Teilzahlungen</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="totalArrivalDebtor">
				<xs:annotation>
					<xs:documentation xml:lang="de">Total (Summe) der Zahlungseingänge durch Schuldner in CHF (nicht gerundet)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="totalArrivalPV">
				<xs:annotation>
					<xs:documentation xml:lang="de">Total (Summe) der Zahlungseingänge durch rückwirkende PV in CHF (nicht gerundet)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="totalAnnulation">
				<xs:annotation>
					<xs:documentation xml:lang="de">Total (Summe) der Rückerstattungen durch Annullation wegen Doppelversicherungen in CHF (nicht gerundet)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="certificateOfLossArrivalType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Eingang Verlustschein</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dossierNr">
				<xs:annotation>
					<xs:documentation xml:lang="de">Eindeutige Dossiernummer des Krankenversicherers zur Betreibung die den Verlustschein ausgelöst hat</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="collectionOffice" type="da-64a-common:collectionOfficeType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betreibungsamt, das den Verlustschein ausgestellt hat</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betreibungsnummer (falls Betreibung existiert)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="certificateOfLoss" type="da-64a-common:certificateOfLossType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Verlustschein oder gleichzusetzender Rechtstitel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtorWithClaim" type="da-64a-common:debtorWithClaimType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Schuldner mit den dazugehörigen Forderungen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="insuredPersonWithClaim" type="da-64a-common:insuredPersonWithClaimType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Versicherte Personen mit den dazugehörigen Forderungen</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="certificateOfLossPaymentType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Zahlungseingang</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dossierNr">
				<xs:annotation>
					<xs:documentation xml:lang="de">Eindeutige Dossiernummer des Krankenversicherer zur Betreibung die den Verlustschein ausgelöst hat</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="certificateOfLoss" type="da-64a-common:certificateOfLossType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Verlustschein oder gleichzusetzender Rechtstitel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtor" type="da-64a-common:debtorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Schuldner (ohne Forderung)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payment" type="da-64a-common:paymentType" maxOccurs="3">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zahlungsart (Zahlung vom Schuldner, rückwirkende PV oder Annullation wegen Doppelversicherung) und zugehörige Summe der Zahlungseingänge für das Dossier in CHF (nicht gerundet)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="certificateOfLossType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Verlustschein</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="lossDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ausstelldatum</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lossNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">
						Verlustscheinnummer (zu beachten: Konkursverlustscheinen enthalten nicht zwingend eine Nummer, daher ist lossNumber optional)
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="dateOfReceipt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Eingangsdatum beim Krankenversicherer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="typeOfLoss">
				<xs:annotation>
					<xs:documentation xml:lang="de">Art des gemeldeten Verlustscheins respektive Rechtstitels</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">Verlustschein</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="de">Konkursverlustschein</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="de">Gleichzusetzender Rechtstitel</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="claimDebtorType">
		<xs:annotation>
			<xs:documentation>Forderung zum Schuldner</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="totalClaim">
				<xs:annotation>
