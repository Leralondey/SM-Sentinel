			<xs:element name="insuredPersonWithClaim" type="da-64a-common:insuredPersonWithClaimType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Versicherte Personen, die nicht betrieben werden dürfen, mit den dazugehörigen Forderungen</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="claimInsuredPersonType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Forderung zur versicherten Person</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="claimAmount">
				<xs:annotation>
					<xs:documentation xml:lang="de">Wert in CHF (nicht gerundet), abzüglich allfälliger Teilzahlungen</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
