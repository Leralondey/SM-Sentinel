	<xs:complexType name="paymentType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Zahlung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="totalAmount">
				<xs:annotation>
					<xs:documentation xml:lang="de">Summe der Zahlungseingänge zum gemeldeten Zahlungstyp in CHF (nicht gerundet)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="paymentCategory">
				<xs:annotation>
					<xs:documentation xml:lang="de">Unterscheidung zwischen Zahlungen vom Schuldner (Betrag wird dem Kanton zu 50% gutgeschrieben) und rückwirkender PV (Betrag wird dem Kanton zu 85 % gutgeschrieben). Ebenfalls gemeldet werden können Annullationen wegen Doppelversicherung</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">Zahlung Schuldner</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="de">Rückwirkende PV</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="de">Annullation wegen Doppelversicherung</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
