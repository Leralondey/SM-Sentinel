				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="interests">
				<xs:annotation>
					<xs:documentation xml:lang="de">Verzugszinsen in CHF (nicht gerundet) abzüglich allfälliger Teilzahlungen</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="expenses">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betreibungskosten in CHF (nicht gerundet) abzüglich allfälliger Teilzahlungen</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="claimDebtorGuaranteedAssumptionType">
