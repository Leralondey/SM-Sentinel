<xs:schema xmlns="http://www.eahv-iv.ch/xmlns/eahv-iv-2403-000102/1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eahv-iv-common="http://www.eahv-iv.ch/xmlns/eahv-iv-common/4" xmlns:eCH-0058="http://www.ech.ch/xmlns/eCH-0058/5" xmlns:eahv-iv-2403-000102="http://www.eahv-iv.ch/xmlns/eahv-iv-2403-000102/1" targetNamespace="http://www.eahv-iv.ch/xmlns/eahv-iv-2403-000102/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.2">
    <xs:annotation>
        <xs:documentation xml:lang="de">Antwort Vertigo</xs:documentation>
        <xs:documentation xml:lang="fr">Réponse Vertigo</xs:documentation>
        <xs:documentation xml:lang="it">Risposta Vertigo</xs:documentation>
    </xs:annotation>
    <xs:import namespace="http://www.eahv-iv.ch/xmlns/eahv-iv-common/4"/>
    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0058/5"/>
    <!--Original Vertigo types-->
    <xs:simpleType name="NameVorname_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">ein Versichertenname oder Versichertenvorname</xs:documentation>
            <xs:documentation xml:lang="fr">Nom ou prénom d'un assuré</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="200"/>
            <xs:pattern value="[A-Za-z]+([\- '][A-Za-z]+)*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Versichertennummer11Stellen_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">11-stellige AHV Nummer</xs:documentation>
            <xs:documentation xml:lang="fr">Numéro AVS à 11 positions</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedLong">
            <xs:pattern value="[1-9][0-9]{10}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Versichertennummer13Stellen_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Schweizerische, 13-stelligen Versichertennummer (756xxxxxxxxxy)</xs:documentation>
            <xs:documentation xml:lang="fr">Numéro AVS suisse à 13 positions (756xxxxxxxxxy)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedLong">
            <xs:minInclusive value="7560000000002"/>
            <xs:maxInclusive value="7569999999998"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Versichertennummer_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Schweizerische, 11- oder 13-stellige Versichertennummer</xs:documentation>
            <xs:documentation xml:lang="fr">Numéro AVS suisse à 11 ou 13 positions </xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="Versichertennummer11Stellen_Type Versichertennummer13Stellen_Type"/>
    </xs:simpleType>
    <xs:simpleType name="Empty_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">leeres Element</xs:documentation>
            <xs:documentation xml:lang="fr">élément vide</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="0"/>
            <!-- xs:pattern value="[0]"/ -->
            <!-- xs:pattern value="\s*"/ -->
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Zero_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Value=0</xs:documentation>
            <xs:documentation xml:lang="fr">Value=0</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0]"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Csex_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Code Geschlecht (1,2)</xs:documentation>
            <xs:documentation xml:lang="fr">Code sexe (1,2)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedByte">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Code_zivilstand_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Code Zivilstand (Infostar)</xs:documentation>
            <xs:documentation xml:lang="fr">Code état civil (Infostar)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[1-9]?"/>
            <!-- peut être vide -->
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Zivilstand_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Zivilstand (Infostar)</xs:documentation>
            <xs:documentation xml:lang="fr">Etat civil (Infostar)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Ledig"/>
            <xs:enumeration value="Verheiratet"/>
            <xs:enumeration value="Verwitwet"/>
            <xs:enumeration value="Geschieden"/>
            <xs:enumeration value="Unverheiratet"/>
            <xs:enumeration value="Eingetragene Partnerschaft"/>
            <xs:enumeration value="Aufgelöste Partnerschaft"/>
            <xs:enumeration value="Unbekannt"/>
            <xs:enumeration value=""/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Fehlercode_Geburtsdatum_numType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Fehlercode Geburtsdatum (0-5)
                0 = TT.MM.JJJJ
                2 = auf den Monat genau
                3 = auf das Jahr genau
                4 = auf die Dekade genau
                5 = 29.02</xs:documentation>
            <xs:documentation xml:lang="fr">Code erreur dans la date de naissance (0-5)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedByte">
            <xs:enumeration value="0"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
            <xs:enumeration value="4"/>
            <xs:enumeration value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Invaliditaetsgrad_numType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Invaliditätsgrad (34-100)</xs:documentation>
            <xs:documentation xml:lang="fr">Degré d'invalidité (34-100)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedByte">
            <xs:minInclusive value="34"/>
            <xs:maxInclusive value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Invaliditaetsgrad_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Invaliditätsmgrad</xs:documentation>
            <xs:documentation xml:lang="fr">Degré d'invalidité</xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="Invaliditaetsgrad_numType Empty_Type"/>
    </xs:simpleType>
    <xs:simpleType name="BetragFR_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Beitrag im AHV-Gebiet : CHF (bis 10^9 CHF)</xs:documentation>
            <xs:documentation xml:lang="fr">Montant monétaire (en CHF) dans le domaine AVS : (jusqu'à 10^9 CHF)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="([\+\-]?\d{1,9})?"/>
            <!-- optional; base="xs:decimal" does not accept empty element -->
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BetragFRRP3_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">3-stelligen Betrag mit Rappen : CHF.RP (bis(3^9).99 CHF)</xs:documentation>
            <xs:documentation xml:lang="fr">Montant monétaire (en CHF.CT) à trois positions plus les décimales : (jusqu'à 999.99 CHF)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:pattern value="([\+\-]?\d{1,3}\.\d{1,2}|\d{1,3}\.\d{1,2})"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BetragFRRP5_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">5-stelligen Betrag mit Rappen : CHF.RP (bis 99999.99 CHF)</xs:documentation>
            <xs:documentation xml:lang="fr">Montant monétaire (en CHF.CT) à six positions plus les décimales : (jusqu'à 99999.99 CHF)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:pattern value="([\+\-]?\d{1,5}\.\d{1,2}|\d{1,5}\.\d{1,2})"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BetragFRRP6_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">6-stelligen Betrag mit Rappen : CHF.RP (bis 999999.99 CHF)</xs:documentation>
            <xs:documentation xml:lang="fr">Montant monétaire (en CHF.CT) à six positions plus les décimales : (jusqu'à 999999.99 CHF)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:pattern value="([\+\-]?\d{1,6}\.\d{1,2}|\d{1,6}\.\d{1,2})"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BetragP6_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">6-stelliger Betrag mit Dezimalangabe : MMMMMM.DD (bis 999999.99)</xs:documentation>
            <xs:documentation xml:lang="fr">Montant monétaire (en MMMMMM.DD) à six positions plus les décimales : (jusqu'à 999999.99)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:pattern value="([\+\-]?\d{1,6}\.\d{1,2}|\d{1,6}\.\d{1,2})"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AHVTimestamp_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Datum und Zeit (automatische Durchführung...)</xs:documentation>
            <xs:documentation xml:lang="fr">Timestamp (d'une opération par ex.)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime">
            <xs:minInclusive value="2004-01-01T00:00:00"/>
            <xs:maxInclusive value="2048-12-31T23:59:59"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AHVDatumJMT_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de"> Genaues datum im AHV-Gebiet (Gültigkeit, Durchführung usw.)</xs:documentation>
            <xs:documentation xml:lang="fr">Date précise (au jour) dans le domaine temporel de l'AVS</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date">
            <xs:minInclusive value="1948-01-01"/>
            <xs:maxInclusive value="2048-12-31"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AHVDatumJM_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Jahr-Monat Datum  im AHV-Gebiet (Gültigkeit, Durchführung usw.)</xs:documentation>
            <xs:documentation xml:lang="fr">Date au mois, dans le domaine de l'AVS</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:gYearMonth">
            <xs:minInclusive value="1948-01"/>
            <xs:maxInclusive value="2048-12"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AHVDatumJ_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Jahr Datum  im AHV-Gebiet (Gültigkeit, Durchführung usw.)</xs:documentation>
            <xs:documentation xml:lang="fr">Date à l'année, dans le domaine de l'AVS</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:gYear">
            <xs:minInclusive value="1948"/>
            <xs:maxInclusive value="2048"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DatumMMJJJJ_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Datum MM.JJJJ</xs:documentation>
            <xs:documentation xml:lang="fr">Date  MM.AAAAA</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{2}\.\d{4}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DatumMMJJJJ_optType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Datum MM.JJJJ (optional)</xs:documentation>
            <xs:documentation xml:lang="fr">AVS MM.AAAAA (optionnel)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\s*|\d{2}\.\d{4}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DatumTTMMJJJJ_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Datum TT.MM.JJJJ</xs:documentation>
            <xs:documentation xml:lang="fr">Date  JJ.MM.AAAAA</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{2}\.\d{2}\.\d{4}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IVStelle_numType">
        <xs:restriction base="xs:unsignedShort">
            <xs:minInclusive value="301"/>
            <xs:maxInclusive value="350"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IVStelle_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">
                Weite Codifizierung der IV-Stellen (default)
            </xs:documentation>
            <xs:documentation xml:lang="fr">
                Offices AI (typage faible, défaut)
            </xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="IVStelle_numType Empty_Type"/>
    </xs:simpleType>
    <xs:simpleType name="Ausgleichskasse_weakType">
        <xs:annotation>
            <xs:documentation xml:lang="de">
                Weite Codifizierung der Ausgleichskassen
            </xs:documentation>
            <xs:documentation xml:lang="fr">
                Codification des caisse de compensation (typage faible, défaut)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedInt">
            <xs:minInclusive value="001000"/>
            <xs:maxInclusive value="516999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="VerfuegungNummer14Stellen_optType">
        <xs:annotation>
            <xs:documentation xml:lang="de">14-stellige Nummer der Verfügung für individuelle Massnahmen</xs:documentation>
            <xs:documentation xml:lang="fr">Numéro de décision à 14 chiffres pour mesures individuelles</xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="xs:unsignedLong Empty_Type"/>
    </xs:simpleType>
    <xs:simpleType name="VerfuegungNummer11Stellen_Type">
        <xs:annotation>
            <xs:documentation xml:lang="de">Numéro de décision à onze chiffres pour mesures individuelles</xs:documentation>
