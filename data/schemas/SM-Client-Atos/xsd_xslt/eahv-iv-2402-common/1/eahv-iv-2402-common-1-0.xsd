            <xs:documentation xml:lang="de">Tätigkeit</xs:documentation>
            <xs:documentation xml:lang="fr">Activité</xs:documentation>
            <xs:documentation xml:lang="it">Attività</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="occupationKind" type="eahv-iv-2402-common:occupationKindType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Erwerbsart</xs:documentation>
                    <xs:documentation xml:lang="fr">Type d'activité lucrative</xs:documentation>
                    <xs:documentation xml:lang="it">Tipo di attività lucrativa</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyAddress" type="eahv-iv-2402-common:companyAddressInformationType"
                minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Firmenadresse</xs:documentation>
                    <xs:documentation xml:lang="fr">Adresse de l'entreprise</xs:documentation>
                    <xs:documentation xml:lang="it">Indirizzo dell'azienda</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="activityStartDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Beginn Tätigkeit - Datumsformat: YYYY-MM-DD (zB.
                        2013-01-30 für 30ter Januar)
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">Début de l'activité - Format de la date: AAAA-MM-JJ
                        (p.ex. 2013-01-30 pour 30 janvier)
                    </xs:documentation>
                    <xs:documentation xml:lang="it">Inizio dell'attività - Formato della data: AAAA-MM-GG
                        (p.es. 2013-01-30 per 30 gennaio)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contractEndDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Ende Arbeitsvertrag - Datumsformat: YYYY-MM-DD (zB.
                        2013-01-30 für 30ter Januar)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="compensationFund" type="eahv-iv-2402-common:institutionType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">AHV-Ausgleichskasse</xs:documentation>
                    <xs:documentation xml:lang="fr">Caisse de compensation AVS</xs:documentation>
                    <xs:documentation xml:lang="it">Cassa di compensazione AVS</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="compensationFundNumber" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Abrechnungsnummer AHV-Ausgleichskasse</xs:documentation>
                    <xs:documentation xml:lang="fr">Numéro de compte de la caisse de compensation AVS
                    </xs:documentation>
                    <xs:documentation xml:lang="it">Numero di conto della cassa di compensazione AVS
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="occupationLocation" type="eahv-iv-2402-common:occupationLocationType"
                maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Erwerbstätigkeiten in folgenden Staaten
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">Activités lucratives dans les Etats suivants
                    </xs:documentation>
                    <xs:documentation xml:lang="it">Attività lucrative nei seguenti Stati</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="infoEmployedType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Angaben zur unselbstständigen Erwerbstätigkeit
            </xs:documentation>
            <xs:documentation xml:lang="fr">Renseignements sur l'activité lucrative salariée
            </xs:documentation>
            <xs:documentation xml:lang="it">Informazioni sull'attività lucrativa dipendente
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="replacesEmployee" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Ersetzt eine andere entsandten Person</xs:documentation>
                    <xs:documentation xml:lang="fr">Remplace une autre personne détachée</xs:documentation>
                    <xs:documentation xml:lang="it">Sostituisce un'altra persona distaccata
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="noticeAuthorityChEmployer" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Kündigungsbefugnis CH Arbeitgeber</xs:documentation>
                    <xs:documentation xml:lang="fr">Droit de l'employeur CH à mettre fin au rapport de
                        travail
                    </xs:documentation>
                    <xs:documentation xml:lang="it">Diritto del datore di lavoro CH di porre fine al
                        rapporto di lavoro
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ordinanceChEmployer" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Tätigkeitbestimmung durch CH Arbeitgeber
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">Maintien du rapport de subordination avec l'employeur CH
                    </xs:documentation>
                    <xs:documentation xml:lang="it">Mantenimento del rapporto di subordinazione con il
                        datore di lavoro CH
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="resumptionOfWorkAtChEmployer" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Wiederaufnahme Arbeit nach Entsendung bei CH Arbeitgeber
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">Reprise du travail pour employeur CH après détachement
                    </xs:documentation>
                    <xs:documentation xml:lang="it">Ripresa del lavoro per datore di lavoro CH dopo distacco
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="workContractParty" type="eahv-iv-2402-common:workContractPartyType" maxOccurs="2">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Arbeitsvertrag mit</xs:documentation>
                    <xs:documentation xml:lang="fr">Contrat de travail avec</xs:documentation>
                    <xs:documentation xml:lang="it">Contratto di lavoro con</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="wagesPaidBy" type="eahv-iv-2402-common:wagesPayerType" maxOccurs="2">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Entlöhnung durch</xs:documentation>
                    <xs:documentation xml:lang="fr">Rémunération par</xs:documentation>
                    <xs:documentation xml:lang="it">Remunerazione da</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="socialinsurancePaidBy" type="eahv-iv-2402-common:socialinsurancePayerType"
                maxOccurs="2">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Sozialversicherungen bezahlt durch</xs:documentation>
                    <xs:documentation xml:lang="fr">Assurances sociales payées par</xs:documentation>
                    <xs:documentation xml:lang="it">Assicurazioni sociali pagate da</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="requestBasedOnMobiliteIntragroup" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Der Antrag basiert auf der Vereinbarung mobilite
                        intragroupe.
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">La demande se base sur l’accord mobilité intragroupe.
                    </xs:documentation>
                    <xs:documentation xml:lang="it">La richiesta si basa sull'accordo mobilité intragroupe.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="infoSelfEmployedType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Angaben zur selbstständigen Erwerbstätigkeit</xs:documentation>
            <xs:documentation xml:lang="fr">Renseignements sur l'activité lucrative indépendante
            </xs:documentation>
            <xs:documentation xml:lang="it">Informazioni sull'attività lucrativa indipendente
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="chInfrastructur" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Hat CH-Infrastruktur</xs:documentation>
                    <xs:documentation xml:lang="fr">La CH a-t-elle une infrastructure</xs:documentation>
                    <xs:documentation xml:lang="it">La Svizzera ha un'infrastruttura?</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="similarJobFunctionAsInCH" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Selbe Tätigkeit wie in CH</xs:documentation>
                    <xs:documentation xml:lang="fr">Même activité qu'en Suisse</xs:documentation>
                    <xs:documentation xml:lang="it">Stessa attività che in Svizzera</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="jobDescription" type="xs:string" minOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Beschreibung der Tätigkeit</xs:documentation>
                    <xs:documentation xml:lang="fr">Description de l'activité</xs:documentation>
                    <xs:documentation xml:lang="it">TODO_IT</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="infoKvgHealthInsuranceType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Angaben zur KVG-Krankenversicherung</xs:documentation>
            <xs:documentation xml:lang="fr">Renseignements sur l'assurance-maladie LAMal</xs:documentation>
            <xs:documentation xml:lang="it">Informazioni sull'assicurazione malattie LAMal
            </xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="kvgHealthInsurance" type="eahv-iv-2402-common:institutionType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">KVG-Krankenversicherer</xs:documentation>
                    <xs:documentation xml:lang="fr">Assureur-maladie LAMal</xs:documentation>
                    <xs:documentation xml:lang="it">Assicuratore malattie LAMal</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="kvgLiberation" type="xs:boolean" fixed="true">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Befreiung KVG</xs:documentation>
                    <xs:documentation xml:lang="fr">Exemption LAMal</xs:documentation>
                    <xs:documentation xml:lang="it">Esenzione LAMal</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
</xs:schema>
