    <!-- ALPS header specifics -->

    <xs:simpleType name="documentTypeType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Dokumenttyp, fachliche Beschreibung des Dokuments.
            </xs:documentation>
            <xs:documentation xml:lang="fr">Type de document, description technique du document.
            </xs:documentation>
            <xs:documentation xml:lang="it">Tipo di documento, descrizione tecnica del documento.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="(01)(\.[0-9][0-9])*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="attachmentType">
        <xs:sequence>
            <xs:element name="title" type="eahv-iv-common:attachmentTitleType" />
            <xs:element name="documentDate" type="eahv-iv-common:documentDateType" />
            <xs:element name="leadingDocument" type="eahv-iv-common:leadingDocumentType" />
            <xs:element name="sortOrder" type="eahv-iv-common:sortOrderType" />
            <xs:element name="documentFormat">
                <xs:simpleType>
                    <xs:restriction base="eahv-iv-common:documentFormatType">
                        <xs:enumeration value="application/pdf" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="documentType" type="eahv-iv-common:documentKindType" />
            <xs:element name="file" type="eahv-iv-common:attachmentFileType" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="extensionType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Attribute, welche im eCH-0058/4-Standard nicht vorgesehen sind,
                werden im Attribut "extension" geführt.
            </xs:documentation>
            <xs:documentation xml:lang="fr">Les attributs qui ne sont pas prévus dans eCH-0058/4-Standard
                doivent être effectués dans l'attribut "extension".
            </xs:documentation>
            <xs:documentation xml:lang="it">Gli attributi che non sono previsti nel eCH-0058/4-Standard
                vengono effettuati nel attributo "extension".
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contactInformation" type="eahv-iv-common:contactInformationType" />
        </xs:sequence>
    </xs:complexType>


