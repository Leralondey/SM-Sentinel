		<xs:annotation>
			<xs:documentation xml:lang="de">Datum im Format Monat-Jahr mit einer Einschränkung, um nicht alles zu erlauben</xs:documentation>
			<xs:documentation xml:lang="fr">Date au format mois-année avec une restriction pour ne pas tout autoriser</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:gYearMonth">
			<xs:minInclusive value="1980-01"/>
			<xs:maxInclusive value="2099-01"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="controlNumberType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Contrainte sur cette valeur pour être représentée en Smallint en DB (32767 étant le max de ce type)</xs:documentation>
		</xs:annotation>
